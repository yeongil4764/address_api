// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Contact {
  id          Int     @default(autoincrement()) @id
  email       String  @unique
  name        String
  age         Int
  phoneNumber String
  description String
}

